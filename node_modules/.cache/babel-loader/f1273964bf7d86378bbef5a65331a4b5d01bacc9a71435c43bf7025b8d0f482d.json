{"ast":null,"code":"import Spinner from './spinner/Spinner3.vue';\nimport NextButton from './button/NextButton.vue';\nimport { mapGetters } from 'vuex';\nexport default {\n  name: \"SecondSection\",\n  components: {\n    Spinner,\n    NextButton\n  },\n  data() {\n    return {\n      value: \"\",\n      loading: false\n    };\n  },\n  computed: {\n    ...mapGetters(['getTopicInfo'])\n  },\n  methods: {\n    async getInfo() {\n      this.loading = true;\n      this.$store.commit('RESET');\n      setTimeout(async () => {\n        const data = await this.$store.dispatch('FETCH_TOPIC_INFO', this.value);\n        if (!data) alert(\"데이터가 존재하지 않습니다!\");\n        this.loading = false;\n      }, 5000);\n    }\n  }\n};","map":{"version":3,"names":["Spinner","NextButton","mapGetters","name","components","data","value","loading","computed","methods","getInfo","$store","commit","setTimeout","dispatch","alert"],"sources":["src/components/Search.vue"],"sourcesContent":["<template>\n  <section class=\"panel spotlight medium right\" id=\"second\">\n    <Spinner :loading=\"loading\"/>\n\n    <NextButton v-if=\"getTopicInfo.length\" :id=\"`#question1`\"/>\n    <div class=\"content span-10 text_center\">\n      <h2 class=\"major\">필요한 지식을 찾아보세요</h2>\n      <div style=\"display:flex; margin-bottom:1rem;\">\n        <input type=\"text\" class=\"bg_gradient_blue\" v-model=\"value\" @keyup.enter=\"getInfo\"/>\n        <a @click=\"getInfo\">\n          <img src=\"images/ic_round-search.svg\" alt=\"Search\" class=\"custom_search\" width=\"24\" height=\"24\" />\n        </a>\n      </div>\n      <p v-if=\"!loading\" style=\"font-size:0.7rem;\">\n        추천 질문: 프론트 개발자가 되고 싶어!, Chat GPT 가 모야?!\n      </p>\n    </div>\n    <div class=\"image filtered tinted\" data-position=\"top left\">\n      <img src=\"images/background_230314.svg\" alt=\"커스텀 배경\" />\n    </div>\n  </section>\n</template>\n<script>\nimport Spinner from './spinner/Spinner3.vue'\nimport NextButton from './button/NextButton.vue'\nimport { mapGetters } from 'vuex'\n\nexport default {\n  name: \"SecondSection\",\n  components: {\n    Spinner,\n    NextButton\n  },\n  data() {\n    return {\n      value: \"\",\n      loading: false\n    }\n  },\n  computed: {\n    ...mapGetters(['getTopicInfo'])\n  },\n  methods: {\n    async getInfo() {\n      this.loading = true\n      this.$store.commit('RESET')\n\n      setTimeout(async () => {\n        const data = await this.$store.dispatch('FETCH_TOPIC_INFO', this.value)\n        if (!data) alert(\"데이터가 존재하지 않습니다!\")\n        this.loading = false\n      }, 5000)\n    }\n  },\n}\n</script>\n<style scoped>\n.content {\n  padding: 9.5rem 3.5rem 2rem 3.5rem !important;\n  -moz-justify-content: unset !important;\n  -webkit-justify-content: unset !important;\n  -ms-justify-content: unset !important;\n  justify-content: unset !important;\n}\n</style>"],"mappings":"AAuBA,OAAAA,OAAA;AACA,OAAAC,UAAA;AACA,SAAAC,UAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAJ,OAAA;IACAC;EACA;EACAI,KAAA;IACA;MACAC,KAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,GAAAN,UAAA;EACA;EACAO,OAAA;IACA,MAAAC,QAAA;MACA,KAAAH,OAAA;MACA,KAAAI,MAAA,CAAAC,MAAA;MAEAC,UAAA;QACA,MAAAR,IAAA,cAAAM,MAAA,CAAAG,QAAA,0BAAAR,KAAA;QACA,KAAAD,IAAA,EAAAU,KAAA;QACA,KAAAR,OAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}